{
  "metadata": [
    {
      "src": [
        {
          "files": "OnTopic.All/bin/Debug/netcoreapp3.1/OnTopic.dll",
          "src": "../"
        }
      ],
      "dest": "api/OnTopic",
      "filter": "Filters/Base.yml"
    },
    {
      "src": [
        {
          "files": "OnTopic.All/bin/Debug/netcoreapp3.1/OnTopic.AspNetCore.Mvc.dll",
          "src": "../"
        }
      ],
      "dest": "api/AspNetCore"
    },
    {
      "src": [
        {
          "files": "OnTopic.All/bin/Debug/netcoreapp3.1/OnTopic.dll",
          "src": "../"
        }
      ],
      "dest": "api/Mapping",
      "filter": "Filters/Mapping.yml"
    },
    {
      "src": [
        {
          "files": [
            "OnTopic.All/bin/Debug/netcoreapp3.1/OnTopic.dll",
            "OnTopic.All/bin/Debug/netcoreapp3.1/OnTopic.Data.*.dll"
          ],
          "src": "../"
        }
      ],
      "dest": "api/Repositories",
      "filter": "Filters/Repositories.yml"
    },
    {
      "src": [
        {
          "files": "OnTopic.All/bin/Debug/netcoreapp3.1/OnTopic.ViewModels.dll",
          "src": "../"
        }
      ],
      "dest": "api/ViewModels"
    },
    {
      "src": [
        {
          "files": "bin/Debug/net6.0/OnTopic.Data.Transfer.dll"
        }
      ],
      "dest": "api/DataTransfer"
    },
    {
      "src": [
        {
          "files": "bin/Debug/net6.0/OnTopic.Editor.AspNetCore.dll"
        }
      ],
      "dest": "api/Editor"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/**/toc.yml",
          "api/**/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appLogoPath": "images/Logo.png"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "repositoryRoot": "../",
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ]
  }
}